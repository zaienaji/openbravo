<REPORT>
  <template file="ModuleBox.srpt"/>
   <structure name="structureBox">
    <FIELD id="name" replaceCharacters="html">name</FIELD>
    <FIELD id="description" replaceCharacters="html">description</FIELD>
    <FIELD id="icon" replace="xx" attribute="class">type</FIELD>
    <FIELD id="help" replaceCharacters="html">help</FIELD>
    <FIELD id="url" attribute="onclick" replace="xx">url</FIELD>
    <FIELD id="url" attribute="style" replace="xx">urlStyle</FIELD>
    <FIELD id="mergeInfo" attribute="style" replace="xx">mergeDisplay</FIELD>
    <FIELD id="mergedModule" replaceCharacters="html">mergedModule</FIELD>
    <FIELD id="mergedInto" replaceCharacters="html">mergedInto</FIELD>
    <FIELD id="mergedIntoVersion" replaceCharacters="html">mergedIntoVersion</FIELD>
    <FIELD id="details" attribute="onclick" replace="xx">moduleVersionID</FIELD>
    <FIELD id="buttonInstall" attribute="onclick" replace="xx">moduleVersionID</FIELD>
    <FIELD id="commercial" attribute="style" replace="xx" default="none">commercialStyle</FIELD>
    <FIELD id="maturitySection" attribute="style" replace="xx" default="none">maturityStyle</FIELD>
    <FIELD id="maturityLevel" default="none">maturityLevel</FIELD>
    <FIELD id="supportSection" attribute="style" replace="xx" default="none">supportStyle</FIELD>
    <FIELD id="support">support</FIELD>
    <SECTION id="box"/>
  </structure>

  <DISCARD id="discard"/>
</REPORT>
